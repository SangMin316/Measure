{
    "dataset": {
        "name": "MASS3",
        "eeg_channel": "F4-LER",
        "num_splits":31,
        "seq_len":1,
        "num_seg": 10,
        "split_len": 512,
        "target_idx": -1,
        "root_dir": "./",
        "samplingrate" : 100
    },

    "backbone": {
        "name": "SleePyCo",
        "init_weights": false,
        "dropout": false
    },

    "Transformer": {
        "zy_dim": 128,
        "dim_feedforward": 128,
        "n_layers": 4,
        "seq_len" : 4
    },

    "feature_pyramid": {
        "dim": 128,
        "num_scales": 3
    },


    "classifier": {
        "name": "Transformer",
        "model_dim": 128,
        "feedforward_dim": 128,
        "pool": "attn",
        "dropout": false,
        "num_classes": 5,
        "nheads" : 8,
        "num_encoder_layers" : 6,
        "pos_enc": {
            "dropout": false
        }
    },

    "proj_head": {
        "name": "Linear",
        "dim": 128
    },

    "training_params": {
        "mode": "Ours_HML",
        "step": "pretrain",
        "max_epochs": 500,
        "batch_size": 1024,
        "lr": 0.0003,        
        "weight_decay": 0.0001,
        "alpha": 0.001,
        "tau": 0.07,
        "val_period": 50,
        "early_stopping": {
            "mode": "min",
            "patience": 20
        }
    }
}


